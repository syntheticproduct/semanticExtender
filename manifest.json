{
    "manifest_version": 2,
    "name": "Stock Tickers",
    "description": "My first Chrome App.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "ticker.png",
      "default_popup": "index.html",
      "default_title": "ticker"
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["mystyles.css"],
        "js": ["content.js", "jquery-3.3.1.slim.min.js"]
      }
    ],

    "permissions": ["activeTab"]
  }